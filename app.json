{
  "expo": {
    "name": "overtime-tracker-rn",
    "slug": "overtime-tracker-rn",
    "owner": "psitsha",
    "scheme": "overtimetracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["android", "ios", "web"],

    "splash": {
      "image": "./assets/logo.png",
      "backgroundColor": "#fffaf3",
      "resizeMode": "contain"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",   // must be square
        "backgroundColor": "#fffaf3"
      },
      "package": "com.psitsha.overtimetrackerrn"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.psitsha.overtimetrackerrn"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-sqlite"
    ],

    "extra": {
      "router": {},
      "eas": { "projectId": "687d137e-6d46-43d1-826e-6ed2ab80aebb" }
    },

    "runtimeVersion": { "policy": "appVersion" },
    "updates": {
      "url": "https://u.expo.dev/687d137e-6d46-43d1-826e-6ed2ab80aebb"
    }
  }
}
